{
  "env": {
    "browser": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "browsers": [
                "> 1%"
              ]
            }
          }
        ],
        "@babel/flow"
      ]
    },
    "server": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "6.11"
            },
            "useBuiltIns": "usage"
          }
        ],
        "@babel/flow"
      ]
    }
  },
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ]
}
